  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "prettify": "prettier --write .",
    "prettify:generated": "yarn prettier --write ./src",
    "tsc": "tsc --noEmit",
    "check:dependencies": "npx depcheck",
    "npm-publish": "shx rm -f .npmrc && npm publish --access=public",
