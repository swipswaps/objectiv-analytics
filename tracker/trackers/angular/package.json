  "devDependencies": {
    "@angular/compiler": "~9.1.13",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@objectiv/tracker-browser": "^0.0.19",
    "ng-packagr": "^9.0.0",
    "prettier": "^2.5.1",
    "rxjs": "~6.5.4",
    "shx": "^0.3.4",
    "tslib": "^1.10.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  }
